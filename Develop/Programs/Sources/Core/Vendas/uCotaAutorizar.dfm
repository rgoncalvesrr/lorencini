  object qServ: TZQuery
    Connection = DM.Design
    SQL.Strings = (
      
        'select true mark, cs.servico, s.descri, cs.qtd, s.un, cs.vvenda,' +
        ' cs.vtotal, cs.recno'
      '  from cota_serv cs '
