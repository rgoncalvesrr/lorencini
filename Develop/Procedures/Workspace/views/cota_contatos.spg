set session authorization lorencini;
create view cota_contatos as         
select co.recno cotacao, cc.cliente, c.recno, c.nome, c.email, c.celular, c.telefone, c.ramal, c.situacao status, cc.funcao, 
        cc.situacao, cc.padrao, cc.obs, c.account 
  from tbclientes_contatos cc       
       join contatos c
         on c.recno = cc.contato 
       join cota co
          on co.cliente = cc.cliente   
 where cc.enviar_cotacao_venda;