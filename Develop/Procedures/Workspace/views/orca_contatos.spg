set session authorization lorencini;
create view orca_contatos as         
select o.recno os, cc.cliente, c.recno, c.nome, c.email, c.celular, c.telefone, c.ramal, c.situacao status, cc.funcao, 
        cc.situacao, cc.padrao, cc.obs, c.account 
  from tbclientes_contatos cc       
       join contatos c
         on c.recno = cc.contato 
       join tb_orcamentos o
          on o.idcliente = cc.cliente   
 where cc.enviar_os;